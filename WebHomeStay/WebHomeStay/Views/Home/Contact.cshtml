<style>
    #map {
        height: 500px;
    }
</style>
<br />
<br />
<div id="map"></div>

<script>
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 10.8061806535468, lng: 106.6286499189848 },
            zoom: 17
        });
        //For HUFI
        var markerDhakaU = new google.maps.Marker({
            position: { lat: 10.8061806535468, lng: 106.6286499189848 },
            map: map
        });
        var contentStringDhakaU = '<div id="content">' +
            '<div id="siteNotice">' +
            '</div>' +
            '<h3 id="firstHeading" class="firstHeading">Trường Đại học Công Nghiệp Thực Phẩm Thành Phố Hồ Chí Minh</h3>' +
            '<div id="bodyContent">' +
            '<p>Trường Đại học Công Nghiệp Thực Phẩm Thành Phố Hồ Chí Minh</br>' +
            '140 Lê Trọng Tấn, Tây Thạnh, Tân Phú, Thành phố Hồ Chí Minh, Việt Nam';
        var infowindowForDhakaU = new google.maps.InfoWindow({
            content: contentStringDhakaU
        });
        markerDhakaU.addListener('mouseover', function () {
            infowindowForDhakaU.open(map, markerDhakaU);
        });
        markerDhakaU.addListener('mouseout', function () {
            infowindowForDhakaU.close();
        });
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDc7PnOq3Hxzq6dxeUVaY8WGLHIePl0swY&callback=initMap" async defer></script>

